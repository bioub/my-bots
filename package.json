{
  "dependencies": {
    "@slack/client": "^5.0.1",
    "axios": "^0.19.0",
    "fs-extra": "^8.0.1",
    "geolib": "^3.0.4",
    "lodash": "^4.17.13",
    "moment": "^2.24.0",
    "node-schedule": "^1.3.2",
    "pluralize": "^8.0.0",
    "puppeteer": "^1.18.0",
    "querystring": "^0.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/fs-extra": "^7.0.0",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.10",
    "@types/pluralize": "0.0.29",
    "@types/puppeteer": "^1.12.4",
    "husky": "^2.5.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "prettier": "prettier --write '{src,config}/**/*.{ts,json}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,json}": [
      "npm run prettier",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "trailingComma": "all",
    "singleQuote": true
  }
}
