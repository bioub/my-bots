{
  "dependencies": {
    "@slack/client": "^4.3.1",
    "axios": "^0.18.0",
    "fs-extra": "^6.0.1",
    "geolib": "^2.0.24",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-schedule": "^1.3.0",
    "pluralize": "^7.0.0",
    "puppeteer": "^1.5.0",
    "querystring": "^0.2.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/fs-extra": "^5.0.3",
    "@types/lodash": "^4.14.110",
    "@types/node": "^8.10.20",
    "@types/pluralize": "0.0.28",
    "@types/puppeteer": "^1.5.0",
    "husky": "^1.0.0-rc.10",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "prettier": "prettier --write 'src/**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier",
      "git add"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "trailingComma": "all",
    "singleQuote": true
  }
}
